    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "concurrently": "^7.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.5.0",
    "file-loader": "^6.2.0",
    "firebase": "^9.6.8",
    "normalize.css": "^8.0.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^5.0.3",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
