{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@google-cloud/firestore": "^5.0.2",
    "@hookform/error-message": "^2.0.0",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "concurrently": "^7.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.5.0",
    "file-loader": "^6.2.0",
    "firebase": "^9.6.8",
    "material-table": "^2.0.3",
    "material-ui-phone-number": "^3.0.0",
    "material-ui-popup-state": "^2.0.0",
    "nanoid": "^3.3.2",
    "normalize.css": "^8.0.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^5.0.3",
    "react-hook-form": "^7.28.0",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "prestart": "kill-port --port 41010",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:ts": "tsc --noEmit",
    "lint:style": "stylelint \"./src/**/*.{css,sss}\"",
    "lint:es": "eslint --format codeframe --cache . --max-warnings=0",
    "lint:prettier": "prettier --list-different .",
    "lint": "concurrently npm:lint:*",
    "lint-fix:es": "eslint --format codeframe --cache . --max-warnings=0 --fix",
    "lint-fix:prettier": "prettier --list-different . --write",
    "lint-fix:ts": "npm run lint:ts",
    "lint-fix:style": "npm run lint:style",
    "lint-fix": "concurrently npm:lint-fix:* -m 1",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.3",
    "@storybook/addon-essentials": "^6.5.3",
    "@storybook/addon-interactions": "^6.5.3",
    "@storybook/addon-links": "^6.5.3",
    "@storybook/builder-webpack5": "^6.5.3",
    "@storybook/manager-webpack5": "^6.5.3",
    "@storybook/node-logger": "^6.5.3",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.5.3",
    "@storybook/testing-library": "^0.0.11",
    "babel-plugin-named-exports-order": "^0.0.2",
    "prop-types": "^15.8.1",
    "webpack": "^5.72.1"
  }
}
